spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/{database-name}
    username: {username}
    password: {password}
    driver-class-name: org.postgresql.Driver
  jpa:
    hibernate:
      ddl-auto: create-drop
    show-sql: true
    properties:
      hibernate:
        format_sql: true
    database: postgresql
    database-platform: org.hibernate.dialect.PostgreSQLDialect
secret-key : {your-secret-key-for-jwt}